<div class="container">
  <div class="row">
    <div class="col-md-4">
      <h1>会員一覧画面（管理者）</h1>
    </div>
  </div>
</div>
<table>
  <thead>
    <tr>
      <td>会員名</td>
      <td>メールアドレス</td>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.screen_name %></td>
      <td><%= customer.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>

