<div class="container">
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-9">
      <h1>自分の投稿一覧</h1>
      <table>
        <thead></thead>
        <tbody>
          <% if @recipes.count == 0 %>
          <tr>
              <h1>投稿がまだありません</h1>
              <% else %>
          </tr>
          <% @recipes.each do |recipe| %>
          <tr>
            <td><%= link_to recipe.recipe_name, recipe_path(recipe.id) %></td>
            <td><%= recipe.recipe_name_previous_change %></td>
            <% end %>
          </tr>
          <% end %>
        </tbody>
      </table>

    </div>
  </div>
</div>
