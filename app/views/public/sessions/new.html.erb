<div class="container">
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-9">
      <h2>ログイン画面</h2>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <table>
        <thead></thead>
        <tbody>
          <tr>
            <td><%= f.label :メールアドレス %></td><td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
          </tr>
          <tr>
            <td><%= f.label :パスワード %></td><td><%= f.password_field :password, autocomplete: "current-password" %></td>
          </tr>
          <tr>
            <td><%= f.submit "ログイン" %></td>
          </tr>
        </tbody>
      </table>
      <% end %>
    </div>
  </div>
</div>







